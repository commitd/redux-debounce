{
  "name": "redux-middleware-debounce",
  "version": "2.0.1",
  "description": "FSA and RSAA compliant middleware for Redux to debounce actions.",
  "license": "MIT",
  "repository": "commitd/redux-middleware-debounce",
  "author": {
    "name": "Jon Elliot",
    "email": "jon@committed.software",
    "url": "http://committed.software"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf .nyc_output coverage lib",
    "coverage": "nyc report -r text-lcov | codecov",
    "lint": "eslint src test",
    "prebuild": "npm run lint",
    "prepublish": "npm run build",
    "pretest": "npm run clean && npm run build",
    "test": "nyc ava",
    "test:watch": "npm test -- --watch"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "files": [
    "lib",
    "license",
    "package.json",
    "readme.md"
  ],
  "keywords": [
    "redux",
    "debounce",
    "middleware",
    "redux-middleware",
    "fsa",
    "flux"
  ],
  "dependencies": {
    "lodash.debounce": "^4.0.6"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "codecov.io": "^0.1.6",
    "eslint": "^3.8.1",
    "eslint-config-react-app": "^0.5.0",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "nyc": "^10.0.0",
    "redux-api-middleware": "^1.0.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3"
  }
}
